{
  "name": "reveal42",
  "description": "Herramientas para ser un estudiante más productivo sin que nuestra cartera sufra ‍🎓",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0 <15.0.0"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "analyze": "nuxt build -a",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "themes": "npm run build:themes && npm run build:highlight:monokai && npm run build:highlight:zenburn && npm run build:highlight:dracula",
    "build:themes": "node-sass -o static/themes assets/css/theme/source --output-style compressed",
    "build:highlight:monokai": "node-sass -o static/themes/highlight node_modules/highlight.js/scss/monokai.scss --output-style compressed",
    "build:highlight:zenburn": "node-sass -o static/themes/highlight node_modules/highlight.js/scss/zenburn.scss --output-style compressed",
    "build:highlight:dracula": "node-sass -o static/themes/highlight node_modules/highlight.js/scss/dracula.scss --output-style compressed"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@mdi/js": "^5.8.55",
    "@nuxtjs/auth-next": "^5.0.0-1602966760.822bc05",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/pwa": "^3.3.1",
    "@nuxtjs/sentry": "^4.5.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "chart.js": "^2.9.4",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "copy-webpack-plugin": "^6.3.2",
    "core-js": "^3.8.0",
    "express-session": "^1.17.1",
    "fontsource-lato": "^3.0.9",
    "fontsource-league-gothic": "^3.0.5",
    "fontsource-montserrat": "^3.0.9",
    "fontsource-news-cycle": "^3.0.9",
    "fontsource-open-sans": "^3.0.9",
    "fontsource-quicksand": "^3.0.3",
    "fontsource-roboto": "^3.0.3",
    "fontsource-source-sans-pro": "^3.0.9",
    "fontsource-ubuntu": "^3.0.9",
    "highlight.js": "^10.4.0",
    "marked": "^1.2.5",
    "minify-css-string": "^1.0.0",
    "nuxt": "^2.14.9",
    "plyr": "3.6.2",
    "redirect-ssl": "^2.0.0",
    "redis": "^3.0.2",
    "socket.io": "^3.0.3",
    "socket.io-client": "^3.0.3",
    "tailwindcss": "^2.0.1",
    "tsparticles": "^1.18.11",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.1",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^3.2.0",
    "autoprefixer": "^10.0.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "eslint-plugin-vue-a11y": "0.0.31",
    "fitty": "^2.3.3",
    "node-sass": "^5.0.0",
    "postcss": "^8.1.10",
    "prettier": "^2.2.1",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "stylelint": "^13.8.0",
